<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/colorDescBackground" />
            <stroke
                android:width="2dp"
                android:color="@color/colorGrayLineForAddPic" />
            <padding
                android:bottom="8dp"
                android:top="8dp"
                android:left="8dp"
                android:right="8dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="line">
            <stroke
                android:width="2dp"
                android:color="@color/colorGrayLineForAddPic" />
        </shape>
    </item>
    <item>
        <rotate
            android:fromDegrees="90"
            android:toDegrees="90">
            <shape android:shape="line">
                <stroke
                    android:width="2dp"
                    android:color="@color/colorGrayLineForAddPic" />
            </shape>
        </rotate>
    </item>
</layer-list>